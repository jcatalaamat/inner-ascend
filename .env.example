NEXT_PUBLIC_URL=http://localhost:3000
# Use NEXT_PUBLIC_URL=https://localhost:3000 if you're running next with --experimental-https
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=OBFUSCATED_KEY
NEXT_PUBLIC_PROJECT_ID=OBFUSCATED_PROJECT_ID

# @see https://docs.expo.dev/guides/environment-variables/
EXPO_PUBLIC_URL=http://localhost:3000
# Use NEXT_PUBLIC_URL=https://localhost:3000 if you're running next with --experimental-https
EXPO_PUBLIC_SUPABASE_URL=http://localhost:54321
EXPO_PUBLIC_SUPABASE_ANON_KEY=OBFUSCATED_KEY

# IMPORTANT: JWT Secret must be at least 32 characters long
SUPABASE_AUTH_JWT_SECRET=super-secret-jwt-token-with-at-least-32-characters-long

# ===================================
# GOOGLE SIGN-IN CONFIGURATION
# ===================================
# See docs/OAUTH_SETUP_GUIDE.md for complete setup instructions
# https://react-native-google-signin.github.io/docs/setting-up/expo

# STEP 1: Get these values from Google Cloud Console (https://console.cloud.google.com)
# - Create OAuth 2.0 Client ID for iOS application type
# - Use bundle ID: com.innerascend.app

# iOS Client ID (from "iOS" application type in Google Cloud Console)
# Format: XXXXXXXXX-YYYYYYYY.apps.googleusercontent.com
GOOGLE_IOS_CLIENT_ID=YOUR_GOOGLE_IOS_CLIENT_ID.apps.googleusercontent.com

# iOS URL Scheme (reverse of iOS Client ID, used for app redirect)
# Format: com.googleusercontent.apps.XXXXXXXXX-YYYYYYYY
GOOGLE_IOS_SCHEME=com.googleusercontent.apps.YOUR_GOOGLE_IOS_CLIENT_ID

# Web Client ID (from "Web application" type in Google Cloud Console)
# Used for: Android sign-in and Supabase token verification
# Format: XXXXXXXXX-YYYYYYYY.apps.googleusercontent.com
GOOGLE_WEB_CLIENT_ID=YOUR_GOOGLE_WEB_CLIENT_ID.apps.googleusercontent.com

# Client Secret (from Web Client ID credentials in Google Cloud Console)
# Used by: Supabase backend for secure token exchange
# SECURITY: Never expose this in client-side code!
GOOGLE_SECRET=YOUR_GOOGLE_SECRET

# STEP 2: Add these credentials to your Supabase project
# Go to: Supabase Dashboard > Authentication > Providers > Google
# - Enable Google provider
# - Enter GOOGLE_WEB_CLIENT_ID as "Client ID"
# - Enter GOOGLE_SECRET as "Client Secret"
# - Add redirect URLs (see docs/OAUTH_SETUP_GUIDE.md)

# Supabase Project ID (for reference, not used in OAuth flow)
NEXT_PUBLIC_SUPABASE_PROJECT_ID=YOUR_SUPABASE_PROJECT_ID
